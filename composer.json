{
  "name": "lgpinstore/rabbitmq-bundle",
  "type": "symfony-bundle",
  "description": "Integrates php-amqplib with Symfony & RabbitMq. Formerly php-amqplib/rabbitmq-bundle, oldsound/rabbitmq-bundle.",
  "keywords": [
    "symfony",
    "symfony4",
    "symfony5",
    "rabbitmq",
    "message",
    "queue",
    "amqp"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alvaro Videla"
    }
  ],
  "require": {
    "php": "^8.3",
    "symfony/dependency-injection": "^5.0|^6.0|^7.0",
    "symfony/event-dispatcher": "^5.0|^6.0|^7.0",
    "symfony/config": "^5.0|^6.0|^7.0",
    "symfony/yaml": "^5.0|^6.0|^7.0",
    "symfony/console": "^5.0|^6.0|^7.0",
    "php-amqplib/php-amqplib": "^2.12",
    "psr/log": "^1.1"
  },
  "require-dev": {
    "symfony/serializer": "^5.0|^6.0|^7.0",
    "symfony/debug": "^4.3|^5.0|^6.0|^7.0",
    "phpunit/phpunit": "^9.5"
  },
  "replace": {
    "oldsound/rabbitmq-bundle": "self.version"
  },
  "suggest": {
    "symfony/framework-bundle": "To use this lib as a full Symfony Bundle and to use the profiler data collector"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.2.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "OldSound\\RabbitMqBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "OldSound\\RabbitMqBundle\\Tests\\": "Tests/"
    }
  }
}
